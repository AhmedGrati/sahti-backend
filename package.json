    "faker": "^5.5.3",
    "handlebars": "^4.7.7",
    "lint-staged": "^12.0.2",
    "multer": "^1.4.4",
    "madge": "^5.0.1",
    "nestjs-url-generator": "^1.0.1",
    "nodemailer": "^6.7.0",
    "nodemon": "^2.0.15",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
